<p id="notice"><%= notice %></p>

<h1 class="exif_data_top"><%= @filter %> | <%= @val %></h1>

<div class="exif_table">

<ul id="quad">
	<% @exif.each do |exif_datum| %>
		<li><%= link_to image_tag(asset_path('thumbnails/'+exif_datum.image.location), class: 'browse_top_block_image'), exif_datum.image %></li>
	<% end %>
</ul>

</div>